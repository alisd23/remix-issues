{
  "private": true,
  "name": "remix-issues",
  "description": "",
  "license": "",
  "scripts": {
    "build": "run-s 'build:*'",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build",
    "dev": "npm run build:server && run-p \"dev:*\"",
    "dev:server": "npm run build:server --watch",
    "dev:remix": "remix dev -c 'node --inspect ./build/server.js'",
    "headless:dev": "remix dev -c 'node --inspect node_modules/.bin/remix-serve ./build/index.js'"
  },
  "dependencies": {
    "@remix-run/express": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@remix-run/web-fetch": "^4.4.1",
    "express": "^4.18.2",
    "react": "^18.*",
    "react-dom": "^18.*"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "@types/express": "^4.17.14",
    "@types/react": "^18.*",
    "@types/react-dom": "^18.*",
    "esbuild": "^0.17.15",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
